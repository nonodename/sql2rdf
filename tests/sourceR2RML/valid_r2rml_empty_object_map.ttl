@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ex: <http://example.com/ns#>.

# The objectMap blank node has rr:unknownProperty rather than any recognised
# R2RML term (rr:column / rr:template / rr:constant / rr:parentTriplesMap).
# buildTermMap() returns nullptr for it, the parser logs a warning, and the
# POM's objectMaps collection must be empty.

<#TriplesMap1>
    rr:logicalTable [ rr:tableName "EMP" ];
    rr:subjectMap [
        rr:template "http://data.example.com/employee/{EMPNO}";
        rr:class ex:Employee;
    ];
    rr:predicateObjectMap [
        rr:predicate ex:name;
        rr:objectMap [ rr:unknownProperty "ENAME" ];
    ].
