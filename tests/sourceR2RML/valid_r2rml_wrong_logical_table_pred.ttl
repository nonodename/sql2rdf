@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ex: <http://example.com/ns#>.

# Valid Turtle and syntactically valid R2RML namespace usage, but the
# predicate inside the logicalTable blank node is rr:table (wrong) rather
# than rr:tableName (correct).  buildLogicalTable() finds neither
# rr:tableName nor rr:sqlQuery, so it must return nullptr.  The TriplesMap
# itself is still identified (it has rr:logicalTable and rr:subjectMap).

<#TriplesMap1>
    rr:logicalTable [ rr:table "EMP" ];
    rr:subjectMap [
        rr:template "http://data.example.com/employee/{EMPNO}";
        rr:class ex:Employee;
    ];
    rr:predicateObjectMap [
        rr:predicate ex:name;
        rr:objectMap [ rr:column "ENAME" ];
    ].
